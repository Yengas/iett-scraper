{
  "name": "iett-scraper",
  "version": "1.0.0",
  "description": "Scraping transportation data from Istanbul Electricity, Tramway and Tunnel General Management website.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build": "tsc",
    "lint": "tslint src/**/*.ts{,x} __tests__/**/*.ts{,x}",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [ "ts", "js" ],
    "moduleDirectories": [ "node_modules", "src" ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/**/*.spec.ts"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yengas/iett-scraper.git"
  },
  "keywords": [
    "istanbul",
    "transportation",
    "iett",
    "scrape",
    "typescript"
  ],
  "author": "Yengas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Yengas/iett-scraper/issues"
  },
  "homepage": "https://github.com/Yengas/iett-scraper#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^10.1.3",
    "jest": "^23.0.1",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3"
  }
}
